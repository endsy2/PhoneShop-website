{
  "version": 2,
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/common/(.*)",
      "dest": "/api/common.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
