{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/common/(.*)",
      "dest": "/api/common/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/admin/$1"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/auth/$1"
    }
  ]
}
